{
    "name": "mirko-pagliai/cakephp-database-backup",
    "description": "Database Backup plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/cakephp-database-backup",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.x-dev",
        "cakephp/chronos": "2.x-dev as 2.0.0",
        "mirko-pagliai/php-tools": "^1.2"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-next",
        "mirko-pagliai/me-tools": "dev-cakephp4",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DatabaseBackup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/test_app/TestApp/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "DatabaseBackup\\Test\\": "tests"
        }
    }
}
